  "legislative_session": "153",
  "identifier": "SCR 32",
  "title": "RECOGNIZING JUNE 19, 2025, AS \"JUNETEENTH NATIONAL INDEPENDENCE DAY\" IN THE STATE OF DELAWARE.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "concurrent resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Resolution proclaims June 19, 2025, as \"Juneteenth National Independence Day\" in Delaware and encourages the people of Delaware to reflect upon the significant roles and many contributions of African Americans, and to celebrate this day with appropriate activities and events."
    }
  ],
