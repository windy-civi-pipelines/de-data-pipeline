  "legislative_session": "153",
  "identifier": "SB 133",
  "title": "AN ACT TO AMEND TITLE 29 OF THE DELAWARE CODE RELATING TO THE DISTRESSED CEMETERY FUND.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Act is a result of the Joint Legislative Oversight and Sunset Committee's review of the Delaware Cemetery Board. Based on the Committee's recommendation, this Act changes the Office of Auditor of Account's obligation to audit the Distressed Cemetery Fund from annually to at least once every 5 years. This Act also updates the name of the Office of Auditor of Accounts."
    }
  ],
  "other_titles": [
    {
      "note": "short title",
      "title": "JLOSC/Delaware Cemetery Board/Audits/1 Year to 5 Years"
    }
  ],
