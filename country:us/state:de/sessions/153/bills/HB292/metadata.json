{
  "legislative_session": "153",
  "identifier": "HB 292",
  "title": "AN ACT TO AMEND TITLE 15 OF THE DELAWARE CODE RELATING TO REVIEW OF CAMPAIGN CONTRIBUTIONS AND EXPENDITURES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
