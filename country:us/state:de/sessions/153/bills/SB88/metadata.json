  "legislative_session": "153",
  "identifier": "SB 88",
  "title": "AN ACT TO AMEND TITLE 21 OF THE DELAWARE CODE RELATING TO RULES OF THE ROAD.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This bill will improve customer service by extending the due date on toll violations from the thirty-first day to the sixty-first day, after the date the violation is transmitted. This bill amends the administrative fee by stating that the fee may be \"up to\" $25."
    }
  ],
  "other_titles": [
