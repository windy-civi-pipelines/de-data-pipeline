  "legislative_session": "153",
  "identifier": "SB 202",
  "title": "AN ACT TO AMEND TITLE 9 OF THE DELAWARE CODE RELATING TO COUNTY PROPERTY TAX DATA.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
