  "identifier": "HCR 70",
  "title": "RECOGNIZING SEPTEMBER 24-27, 2025, AS \u201cFIRST RESPONDER SUICIDE AWARENESS DAYS\u201d.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "concurrent resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This House Concurrent Resolution recognizes September 24, 2025, as Firefighter Suicide Awareness Day, September 25, 2025, as Emergency Medical Services Suicide Awareness Day, September 26, 2025, as Law Enforcement Suicide Awareness Day and September 27, 2025, as 911 Telecommunications Suicide Awareness Day, in Delaware."
    }
  ],
