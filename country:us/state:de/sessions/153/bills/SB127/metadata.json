  "legislative_session": "153",
  "identifier": "SB 127",
  "title": "AN ACT TO AMEND THE CHARTER OF THE CITY OF NEW CASTLE RELATING TO RENTS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
