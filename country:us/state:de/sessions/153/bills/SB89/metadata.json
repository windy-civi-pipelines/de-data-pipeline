{
  "legislative_session": "153",
  "identifier": "SB 89",
  "title": "AN ACT TO AMEND TITLE 6 OF THE DELAWARE CODE RELATING TO CREDIT CARD TRANSACTIONS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Act prohibits a seller that regularly accepts payment by credit card from (1) imposing a credit card surcharge greater than the percentage processing fee charged by the credit card company, for transactions of $1,500 or less, and (2) refusing to accept payment by credit card or imposing any credit card surcharge, for transactions that exceed $1,500."
    }
  ],
