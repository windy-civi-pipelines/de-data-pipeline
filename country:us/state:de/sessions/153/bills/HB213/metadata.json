  "legislative_session": "153",
  "identifier": "HB 213",
  "title": "AN ACT TO AMEND TITLE 10 OF THE DELAWARE CODE RELATING TO EVIDENCE AND WITNESSES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
