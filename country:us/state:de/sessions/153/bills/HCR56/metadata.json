  "identifier": "HCR 56",
  "title": "RECOGNIZING MAY 20, 2025, AS \"DIRECT SUPPORT PROFESSIONAL\" ADVOCACY DAY.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "concurrent resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Concurrent Resolution recognizes the historic sacrifices and ongoing contributions of Delaware's Direct Support Professionals serving vulnerable adults with intellectual and developmental disabilities. It also recognizes importance and expertise of Direct Support Professionals to our state and establishes May 20th as \u201cDirect Support Professional\u201d Advocacy Day sponsored by the Ability Network of Delaware and A-Team Delaware."
    }
  ],
