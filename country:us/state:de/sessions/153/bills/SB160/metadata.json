  "identifier": "SB 160",
  "title": "AN ACT TO AMEND TITLE 14 OF THE DELAWARE CODE RELATING TO PROVIDING INFORMATION ON TYPE 1 DIABETES.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Act requires the Department of Health and Social Services, in consultation with the Department of Education, to post existing materials on type 1 diabetes to their website.  Each local education agency shall make the informational materials available to the parents or guardians of students enrolled in their schools."
    }
  ],
