{
  "legislative_session": "153",
  "identifier": "HCR 68",
  "title": "REAFFIRMING THE LONGSTANDING SISTERHOOD PARTNERSHIP BETWEEN DELAWARE AND TAIWAN.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "concurrent resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This House Concurrent Resolution reaffirms Delaware's commitment to strengthen the longstanding partnership with Taiwan, supports Taiwan\u2019s efforts to secure the signing of a Bilateral Trade Agreement and an Avoidance of Double Taxation Agreement with the United States; commends Taiwan\u2019s vibrant full-fledged democracy; supports the reopening of a Delaware Trade Office in Taiwan; and continues to support Taiwan\u2019s meaningful participation and contributions in international organizations."
    }
  ],
